{
  "name": "snapdragon-cheerio",
  "description": "Snapdragon plugin for converting a cheerio AST to a snapdragon AST.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/snapdragon-cheerio",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/snapdragon-cheerio",
  "bugs": {
    "url": "https://github.com/jonschlinkert/snapdragon-cheerio/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "define-property": "^0.2.5",
    "extend-shallow": "^2.0.1",
    "is-self-closing": "^1.0.0",
    "snapdragon-node": "^1.0.3",
    "snapdragon-util": "^1.0.3"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0",
    "snapdragon": "^0.9.1"
  },
  "keywords": [
    "html",
    "ast",
    "nodes",
    "cheerio",
    "compile",
    "compiler",
    "convert",
    "parse",
    "parser",
    "render",
    "snapdragon",
    "transform"
  ],
  "verb": {
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "cheerio",
        "sledgehammer",
        "snapdragon",
        "snapdragon-capture",
        "snapdragon-capture-set",
        "snapdragon-node",
        "snapdragon-util"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
